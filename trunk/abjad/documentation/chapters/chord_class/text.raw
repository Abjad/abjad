<!-- ### ARTICULATIONS ### -->

<div class="example">
<p class="example-header-inherited"> articulations
<pre>
   abjad> chord = Chord([0, 2, 10], (1, 4))
   abjad> measure = Measure((1, 4), [chord])
   abjad> measure.meter.transparent = True
   abjad> chord.articulations.append('staccato')
   abjad> show(measure)
   abjad> print(chord.format)
   &lt;c' d' bf'&gt;4 -\staccato
</pre>
</div>

<div class="example">
<p class="example-header-inherited"> barline
<pre>
   abjad> chord = Chord([0, 2, 10], (1, 4))
   abjad> measure = Measure((1, 4), [chord])
   abjad> measure.meter.transparent = True
   abjad> chord.barline = '||'
   abjad> show(measure)
   abjad> print(chord.format)
   &lt;c' d' bf'&gt;4
   \bar "||"
</pre>
</div>

<div class="example">
<p class="example-header-inherited"> beam
<pre>
   abjad> chord = Chord([0, 2, 10], (1, 4))
   abjad> measure = Measure((1, 4), [chord])
   abjad> measure.meter.transparent = True
   abjad> show(measure)
   abjad> chord.beam.beamable
   False
</pre>
</div>

